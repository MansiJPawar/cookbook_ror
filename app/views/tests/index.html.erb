<p style="color: green"><%= notice %></p>


<div id="tests" class="container">

<h1>Tests</h1>
  
  <% @tests.each do |test| %>
    <%= render test %>
    <p>
      <%= link_to "Show this test", test , class:"btn btn-primary btn-sm" %>
    </p>
  <% end %>
  <%= link_to "New test", new_test_path , class:"btn btn-primary btn-sm" %>
</div>


